import sqlite3

class dbsql:
    def __init__(self):
        pass

    def ingestCandles(pair,candles):
        con = sqlite3.connect("datasets.db")
        cur = con.cursor()
        cur.execute(f"CREATE TABLE IF NOT EXISTS {pair} (start INTEGER PRIMARY KEY, low REAL, high REAL, open REAL, close REAL, volume REAL)")
        
        for candle in candles:
            cur.execute(f"INSERT INTO movie VALUES({candle['start']}, {candle['low']}, {candle['high']}, {candle['open']}, {candle['close']}, {candle['volume']})")
        return